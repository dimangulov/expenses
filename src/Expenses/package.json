{
  "name": "expenses",
  "version": "1.0.0-rc3",
  "scripts": {
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "test:ci": "npm run test -- --runInBand",
    "test:coverage": "npm run test -- --coverage",
    "build:dev": "webpack --progress --color",
    "build:aot": "webpack --env.aot --env.client & webpack --env.aot --env.server",
    "test:e2e": "tsc --project e2e/ & protractor e2e\\e2e.config.js"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "./Client/test/setupJest.ts",
    "globals": {
      "__TS_CONFIG__": "Client/tsconfig.spec.json",
      "__TRANSFORM_HTML__": true
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/Client/test/.*.ts"
    ],
    "coverageDirectory": "coverage"
  },
  "dependencies": {
    "@angular/animations": "^4.1.3",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/platform-server": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@ngx-translate/core": "^6.0.1",
    "@ngx-translate/http-loader": "0.0.3",
    "@progress/kendo-angular-dateinputs": "^1.0.1",
    "@progress/kendo-angular-dropdowns": "^1.0.9",
    "@progress/kendo-angular-excel-export": "^1.0.1",
    "@progress/kendo-angular-grid": "^1.1.2",
    "@progress/kendo-angular-inputs": "^1.0.3",
    "@progress/kendo-angular-intl": "^1.0.0",
    "@progress/kendo-angular-l10n": "^1.0.0",
    "@progress/kendo-data-query": "^1.0.1",
    "@progress/kendo-drawing": "^1.0.2",
    "@progress/kendo-theme-default": "^2.37.0",
    "@types/node": "^7.0.12",
    "angular2-template-loader": "0.6.0",
    "aspnet-prerendering": "2.0.3",
    "aspnet-webpack": "^1.0.17",
    "awesome-typescript-loader": "^3.0.0",
    "bootstrap": "^3.3.7",
    "bootstrap-sass": "^3.3.7",
    "core-js": "^2.4.1",
    "css": "^2.2.1",
    "css-loader": "^0.25.0",
    "event-source-polyfill": "^0.0.7",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^2.0.0-rc",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.3.1",
    "json-loader": "^0.5.4",
    "moment": "^2.22.2",
    "ng2-bootstrap": "^1.6.1",
    "ng2-signalr": "^2.0.2",
    "node-sass": "^4.8.1",
    "preboot": "^4.5.2",
    "primeng": "^4.0.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "rxjs": "^5.0.1",
    "sass-loader": "^6.0.3",
    "signalr": "^2.2.1",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.5",
