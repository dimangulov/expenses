    "chai": "^3.5.0",
    "codelyzer": "^3.0.0-beta.4",
    "jasmine-core": "^2.5.2",
    "jest": "^20.0.0",
    "jest-preset-angular": "^2.0.1",
    "protractor": "^5.1.2",
    "tslint": "^4.5.1"
  }
}
